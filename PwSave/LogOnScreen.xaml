<Window x:Class="PwSave.LogOnScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PwSave"
        mc:Ignorable="d"
        Title="Eingabe: Passwort" Height="163.194" Width="400" WindowStartupLocation="CenterScreen" Icon="circle-yellow-sm.png" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Passwort" HorizontalAlignment="Left" Margin="48,24,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="tbPasswort" HorizontalAlignment="Left" Height="23" Margin="48,49,0,0" VerticalAlignment="Top" Width="290"/>
        <Button x:Name="btnAbbrechen" Content="Abbrechen" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="75" Click="btnAbbrechen_Click"/>
        <Button x:Name="btnGo" Content="OK" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="btnGo_Click" IsDefault="True"/>
    </Grid>
</Window>
